{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@pages/*": ["./src/App/pages/*"],
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@config/*": ["./src/config/*"],
      "@context/*": ["./src/context/*"],
      "@helpers/*": ["./src/helpers/*"],
      "@router/*": ["./src/router/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@translation/*": ["./src/assets/translation/*"],
      "@i18n/*": ["./src/i18n/*"],
      "@layout/*": ["./src/layout/*"],
      "@type/*": ["./src/types/*"],
      "@interface/*": ["./src/types/interface/*"],
      "@public/*": ["./public/*"],
      "@store/*": ["./store/*"]
    }
  }
}
